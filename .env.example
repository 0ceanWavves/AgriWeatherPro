# API Keys
REACT_APP_OPENWEATHERMAP_API_KEY=your-openweathermap-api-key

# Feature Flags
REACT_APP_USE_REAL_MAPS=true
REACT_APP_USE_MOCK_DATA=false

# Supabase Configuration
REACT_APP_SUPABASE_URL=your-supabase-project-url
REACT_APP_SUPABASE_ANON_KEY=your-supabase-anon-key
REACT_APP_SUPABASE_SERVICE_ROLE=your-supabase-service-role-key

# Vite Environment Variables (if you're using Vite)
VITE_SUPABASE_URL=your-supabase-project-url
VITE_SUPABASE_ANON_KEY=your-supabase-anon-key

# Note: Never commit the actual .env file with real values to version control
# Make sure .env is listed in .gitignore
